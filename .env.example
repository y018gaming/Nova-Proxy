# Server Configuration
PORT=8080
HOSTNAME=0.0.0.0

# Proxy Configuration
BARE_PATH=/bare/
PROXY_PATH=/proxy/

# Game Emulation
ENABLE_GAME_EMULATION=true

# GitHub Codespaces Support
# These will be automatically set when running in Codespaces
# CODESPACES=true
# CODESPACE_PORT=8080

# SSL Configuration (for HTTPS)
# Uncomment and set these values to enable HTTPS
# SSL=true
# SSL_KEY_PATH=./ssl/key.pem
# SSL_CERT_PATH=./ssl/cert.pem

# Analytics (optional)
# ANALYTICS_ENABLED=false
